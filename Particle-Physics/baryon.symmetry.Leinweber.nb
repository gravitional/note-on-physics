  RowBox[{
   FractionBox["1", "3"], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"\[CapitalSigma]0`u", "[", "x", "]"}]}], "+", 
     RowBox[{"2", 
      RowBox[{"\[CapitalSigma]0`d", "[", "x", "]"}]}], "-", 
     RowBox[{"\[CapitalSigma]0`s", "[", "x", "]"}]}], ")"}], 
   " "}]}]], "DisplayFormula",
